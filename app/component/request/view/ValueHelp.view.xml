<mvc:View
    controllerName="project1.component.request.controller.ValueHelp"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">

    <Page title="물품 요청 생성">
        <content>
            <Input
                id="cityInput"
                placeholder="물품 선택"
                showValueHelp="true"
                valueHelpRequest="onValueHelpRequest" />
            <Button text="Submit" press="onSubmit" />
        </content>
    </Page>

</mvc:View>